{
  "name": "n8n-nodes-salesforce-bulk",
  "version": "0.1.15",
  "description": "salesforce bulk api adapter for n8n",
  "keywords": [
    "n8n-community-node-package", "salesforce", "bulk api"
  ],
  "license": "MIT",
  "homepage": "https://github.com/tsschnoc/n8n-nodes-salesforce-bulk.git",
  "author": {
    "name": "dev",
    "email": "dev@example.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tsschnoc/n8n-nodes-salesforce-bulk.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes --write",
    "lint": "eslint nodes package.json",
    "lintfix": "eslint nodes package.json --fix",
    "prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes package.json"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [],
    "nodes": [
      "dist/nodes/SalesforceBulk/SalesforceBulk.node.js"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/request-promise-native": "~1.0.15",
    "@typescript-eslint/parser": "~5.45",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "gulp": "^4.0.2",
    "n8n-core": "*",
    "n8n-workflow": "*",
    "prettier": "^2.7.1",
    "typescript": "~4.8.4"
  },
  "dependencies": {
    "jsonwebtoken": "^9.0.2"
  }
}
